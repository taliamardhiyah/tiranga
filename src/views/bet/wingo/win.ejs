<script>
  function animateText(text, containerId, colors, delay = 200) {
    const container = document.getElementById(containerId);
    container.innerHTML = ""; // Hapus konten lama jika ada

    text.split("").forEach((char, index) => {
      const span = document.createElement("span");
      span.textContent = char;
      span.classList.add("char");
      span.style.color = colors[index % colors.length]; // Warna bergantian
      container.appendChild(span);

      setTimeout(() => {
        span.style.opacity = 1;
      }, index * delay);
    });

    setInterval(() => {
      document.querySelectorAll(`#${containerId} .char`).forEach((char) => {
        char.style.opacity = Math.random() > 0.5 ? "1" : "0.5"; // Efek kelap-kelip
      });
    }, 500);
  }

  const colors = ["#FFD700", "#FF4500", "#1E90FF", "#32CD32", "#FF69B4", "#8A2BE2"];
  animateText("Pasificrim Wingo", "animatedText", colors);
</script>

