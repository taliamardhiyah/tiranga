<script>
    document.addEventListener("DOMContentLoaded", function () {
        const withdrawalList = document.getElementById("withdrawal-list");
        const maxItems = 6;
        const paymentMethods = ["BCA", "Mandiri", "BNI", "BRI", "NeoBank", "OVO", "Dana", "GOPAY", "Bank Jago", "SEABANK"];
        const withdrawalAmounts = [
            20000, 90000, 150000, 1000000, 15000000, 1500000, 25000000, 10000, 15000, 768212, 562100, 425000, 232123, 
            34563, 287634, 1983200, 1300000, 1221000, 113000, 25000, 38000, 40000, 42000, 45000, 30000, 48000, 15200, 18320, 97500, 82000, 76000, 55000, 45000, 31230, 27500, 21300, 124000, 111000, 195000,
            50000, 55000, 60000, 65000, 75000, 70000, 80000, 82000, 85000, 90000, 125000, 100000, 120000, 150000, 
            200000, 250000, 300000, 220000, 500000, 105000, 135000, 160000, 232000, 231000, 425000, 700000, 1000000, 
            1500000, 2000000, 3000000, 5000000, 10000000, 12000000, 15000000
        ];

        function getSeededRandom(seed) {
            let x = Math.sin(seed) * 10000;
            return x - Math.floor(x);
        }

        function generateRandomWithdrawal(index) {
            const now = new Date();
            const formattedTime = now.toLocaleString("id-ID", { 
                hour: "2-digit", minute: "2-digit", second: "2-digit",
                day: "2-digit", month: "2-digit", year: "numeric"
            });

            const baseSeed = Math.floor(now.getTime() / 10000); 
            const seed = baseSeed + index * 13;

            const userIndex = Math.floor(getSeededRandom(seed) * 8000) + 1000;
            const paymentIndex = Math.floor(getSeededRandom(seed + 1) * paymentMethods.length);
            const amountIndex = Math.floor(getSeededRandom(seed + 2) * withdrawalAmounts.length);

            return {
                username: `Mem***${userIndex}`,
                amount: withdrawalAmounts[amountIndex],
                payment: paymentMethods[paymentIndex],
                time: formattedTime
            };
        }

        function updateWithdrawals() {
            if (withdrawalList.children.length >= maxItems) {
                withdrawalList.removeChild(withdrawalList.firstChild);
            }

            const newWithdrawal = generateRandomWithdrawal(withdrawalList.children.length);
            const listItem = document.createElement("div");
            listItem.classList.add("withdrawal-item");
            listItem.innerHTML = `
                <span>‚úÖ <strong>${newWithdrawal.username}</strong> Menarik Dana, 
                <strong>${newWithdrawal.payment}</strong> <br>
                <strong>Rp${newWithdrawal.amount.toLocaleString()}</strong> <br>
                <small>${newWithdrawal.time}</small></span>
            `;

            withdrawalList.appendChild(listItem);
        }

        for (let i = 0; i < maxItems; i++) {
            updateWithdrawals();
        }

        setInterval(updateWithdrawals, 10000);
    });
</script>

